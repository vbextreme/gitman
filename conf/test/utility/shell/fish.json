{
	"repository":"https://github.com/fish-shell/fish-shell.git",
	"name":"fish-shell",
	"tag":"",
	"branch":"master",
	"dependencies":{
		"fedora":["ncurses-dev"],
		"debian":["libncurses5-dev","autoconf*"],
		"ubuntu":["libncurses5-dev"]
	},
	"build": {
		"default":[
			"autoreconf --no-recursive",
			"./configure",
			"make -j$DEFAULT_JOBS"
			]
	},
	"install":{
		"common":["make install"]
	}
}
